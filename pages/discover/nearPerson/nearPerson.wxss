
page{
  height:100%;
}


.container {
  padding: 0;
  align-items:stretch;
  
}

.content{
  flex: 1 1 auto;
}

.filter{
  display: flex;
  flex: 1 1 auto;
  justify-content: center;
  padding-bottom: 5px;
}

.filterText{
  padding-right: 1px;
  flex: 0 0 40px;
}

.filterContent{
  text-align: center;
  flex: 1 1 auto;
}

.healthData{
  flex: 1 1 auto;
}

.table{
    border:1px solid #dadada;
    border-right: 0;
    border-bottom:0;
    width: 100%; 
    align-self: flex-end;
    flex: 0 0 40px;
}
.tr{
   width: 100%; 
   display: flex;
   /* justify-content:space-between; */
   flex-wrap: nowrap; 
} 

.th,.td {
  /* padding: 10px; */
  border-bottom: 1px solid #dadada;
  border-right: 1px solid #dadada;
  text-align: center;
   /* width:100%;  */
  flex:1 0 auto;
  font: 18px/15px arial;  
  color:#515151;
  height: 40px;
  line-height: 40px;
}
.th {
  font-weight: 400;
  background-color: #dadada
}